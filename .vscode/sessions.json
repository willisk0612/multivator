{
    "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
    "theme": "tribe",
    "active": "elev 2",
    "activateOnStartup": true,
    "keepExistingTerminals": false,
    "sessions": {
        "default": [
            [
                {
                    "name": "Simulator 1",
                    "autoExecuteCommands": true,
                    "commands": [
                        "cd lib/simulator && ./SimElevatorServer.exe"
                    ]
                },
                {
                    "name": "main 1",
                    "autoExecuteCommands": true,
                    "commands": [
                        "go run src/main.go --id 0"
                    ]
                }
            ]
        ],
        "elev 2": [
            [
                {
                    "name": "Simulator 2",
                    "autoExecuteCommands": true,
                    "commands": [
                        "cd lib/simulator && ./SimElevatorServer.exe --port 15658"
                    ]
                },
                {
                    "name": "main 2",
                    "autoExecuteCommands": true,
                    "commands": [
                        "go run src/main.go --id 1"
                    ]
                }
            ]
        ],
        "elev 3": [
            [
                {
                    "name": "Simulator 3",
                    "autoExecuteCommands": true,
                    "commands": [
                        "cd lib/simulator && ./SimElevatorServer.exe --port 15659"
                    ]
                },
                {
                    "name": "main 3",
                    "autoExecuteCommands": true,
                    "commands": [
                        "go run src/main.go --id 2"
                    ]
                }
            ]
        ],
        "elev 1 race": [
            [
                {
                    "name": "Simulator 1",
                    "autoExecuteCommands": true,
                    "commands": [
                        "cd lib/simulator && ./SimElevatorServer.exe"
                    ]
                },
                {
                    "name": "main 1 race",
                    "autoExecuteCommands": true,
                    "commands": [
                        "go run -race src/main.go --id 0"
                    ]
                }
            ]
        ],
        "elev 2 race": [
            [
                {
                    "name": "Simulator 2",
                    "autoExecuteCommands": true,
                    "commands": [
                        "cd lib/simulator && ./SimElevatorServer.exe --port 15658"
                    ]
                },
                {
                    "name": "main 2 race",
                    "autoExecuteCommands": true,
                    "commands": [
                        "go run -race src/main.go --id 1"
                    ]
                }
            ]
        ],
        "elev 3 race": [
            [
                {
                    "name": "Simulator 3",
                    "autoExecuteCommands": true,
                    "commands": [
                        "cd lib/simulator && ./SimElevatorServer.exe --port 15659"
                    ]
                },
                {
                    "name": "main 3 race",
                    "autoExecuteCommands": true,
                    "commands": [
                        "go run -race src/main.go --id 2"
                    ]
                }
            ]
        ]
    }
}